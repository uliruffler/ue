syntax toml *.toml
# Comments (entire line including the # sign)
color cyan "^[[:space:]]*#.*$"
# Section headers [section]
color brightmagenta "^\[.*\]$"
# Keys (before the = sign)
color brightgreen "^[[:space:]]*[A-Za-z_][A-Za-z0-9_]*[[:space:]]*="
# Boolean values
color brightyellow "\b(true|false)\b"
# Numbers
color brightyellow "\b[0-9]+\b"
# Strings (quoted values)
color yellow "\"[^\"]*\""

